{
    "name": "n8n-nodes-bozonx-free-llm-router-microservice",
    "version": "1.0.0",
    "description": "n8n node for Free LLM Router Microservice",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "llm",
        "router",
        "ai"
    ],
    "license": "MIT",
    "homepage": "https://github.com/bozonx/free-llm-router-microservice",
    "author": {
        "name": "bozonx",
        "email": ""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bozonx/free-llm-router-microservice.git"
    },
    "main": "index.js",
    "scripts": {
        "build": "tsc && gulp build:icons",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "lint": "eslint nodes credentials package.json",
        "lintfix": "eslint nodes credentials package.json --fix",
        "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/FreeLlmRouterApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/FreeLlmRouter/FreeLlmRouter.node.js"
        ]
    },
    "devDependencies": {
        "@langchain/core": "^0.3.0",
        "@langchain/openai": "^0.3.0",
        "@types/node": "^22.0.0",
        "@typescript-eslint/parser": "^7.15.0",
        "eslint": "^8.56.0",
        "eslint-plugin-n8n-nodes-base": "^1.16.1",
        "gulp": "^4.0.2",
        "n8n-workflow": "*",
        "prettier": "^3.3.2",
        "typescript": "^5.5.3"
    },
    "peerDependencies": {
        "@langchain/core": "*",
        "@langchain/openai": "*",
        "n8n-workflow": "*"
    }
}