{
  "name": "free-llm-router-microservice",
  "version": "1.1.0",
  "description": "Free LLM Router Microservice with multi-provider fallback",
  "type": "module",
  "main": "dist/src/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main.js",
    "start:dev": "NODE_ENV=development node --watch src/main.ts",
    "start:prod": "NODE_ENV=production node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules' jest --selectProjects unit",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:cov": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:debug": "node --inspect-brk --loader ts-node/esm node_modules/.bin/jest --runInBand --detectOpenHandles",
    "test:unit:debug": "node --inspect-brk --loader ts-node/esm node_modules/.bin/jest --runInBand --selectProjects unit --detectOpenHandles",
    "test:e2e:debug": "node --inspect-brk --loader ts-node/esm node_modules/.bin/jest --runInBand --selectProjects e2e --detectOpenHandles",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules' jest --selectProjects e2e",
    "update-models": "ts-node scripts/fetch-models.ts > models.yaml"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "hono": "^4.9.1",
    "js-yaml": "^4.1.1",
    "pino": "^10.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "nock": "^14.0.10",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.7.4",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  },
  "packageManager": "pnpm@10.29.3"
}