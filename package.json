{
  "name": "free-llm-router-microservice",
  "version": "1.0.0",
  "description": "Free LLM Router Microservice with multi-provider fallback",
  "type": "module",
  "main": "dist/src/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules' jest --selectProjects unit",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:cov": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:debug": "node --inspect-brk --loader ts-node/esm node_modules/.bin/jest --runInBand --detectOpenHandles",
    "test:unit:debug": "node --inspect-brk --loader ts-node/esm node_modules/.bin/jest --runInBand --selectProjects unit --detectOpenHandles",
    "test:e2e:debug": "node --inspect-brk --loader ts-node/esm node_modules/.bin/jest --runInBand --selectProjects e2e --detectOpenHandles",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules' jest --selectProjects e2e",
    "update-models": "ts-node scripts/fetch-models.ts > models.yaml"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-fastify": "^11.1.9",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "js-yaml": "^4.1.1",
    "nestjs-pino": "^4.5.0",
    "pino": "^10.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@jest/globals": "^30.2.0",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.2.1",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "nock": "^14.0.10",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "author": "Ivan K",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.25.0"
}